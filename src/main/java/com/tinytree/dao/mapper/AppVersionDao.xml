<mapper namespace="com.kungfu.dental.dao.AppVersionDao"> 
	
	<select id = "getLatestAppVersion" resultMap = "appVersionMap">
	SELECT
	*
	FROM
	app_version
	WHERE
	create_date = (
		SELECT
			max(create_date) AS create_date
		FROM
			app_version
		WHERE
			app_type = #{0} 
			AND app_name= #{1}  
		AND status = 1 )
	</select>
