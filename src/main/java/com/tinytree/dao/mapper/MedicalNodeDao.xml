<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 
         符号转义说明
    &lt;          < 
    &gt;          >  
    &lt;&gt;     <>
    &amp;        & 
    &apos;       '
    &quot;       "
  <![CDATA[ 这里写你的SQL或者符号 ]]> 
 -->
<mapper namespace="com.kungfu.dental.dao.MedicalNodeDao"> 
	<select id = "getParentNode" resultMap = "medicalNodeMap">
		SELECT * 
		FROM medical_node
		WHERE id = 
		(SELECT parent_node_id 
		FROM medical_node
		WHERE id = #{0})
	</select>
	<select id = "getChildNodes" resultType = "java.util.HashMap">
		SELECT id AS nodeId,
		node_name AS nodeName,
		node_type AS nodeType,
		template_id AS templateId
		FROM medical_node
		WHERE parent_node_id = #{0}
	</select>
	<select id = "getRootNodes" resultType = "java.util.HashMap">
		SELECT id AS nodeId,
		node_name AS nodeName,
		node_type AS nodeType,
		template_id AS templateId
		FROM medical_node
		WHERE node_level = 0
	</select>
	<resultMap type="com.kungfu.dental.entity.MedicalNode" id="medicalNodeMap">
		<id column="id" property="id"/>
		<result column="parent_node_id" property="parentNodeId"/>
		<result column="node_name" property="nodeName"/>
		<result column="node_type" property="nodeType"/>
		<result column="node_level" property="nodeLevel"/>
		<result column="template_id" property="templateId"/>
		<result column="create_date" property="createDate"/>
		<result column="modify_date" property="modifyDate"/>
	</resultMap>
	<!-- tbaleColumns  所有列 -->
  	<sql id="tbaleColumns">
		<trim suffix="" suffixOverrides=",">
			id,parent_node_id,node_name,node_type,node_level,template_id,create_date,modify_date		
		</trim>
	</sql>
	
	<!-- insertColumns 入库列 -->
	<sql id="insertColumns">
		<trim suffix="" suffixOverrides=",">
			<if test="id != null and id != '' " >
				id,
			</if>
			<if test="parentNodeId != null and parentNodeId != '' " >
				parent_node_id,
			</if>
			<if test="nodeType != null " >
				node_type,
			</if>
			<if test="nodeLevel != null " >
				node_level,
			</if>
			<if test="nodeName != null and nodeName != ''" >
				node_name,
			</if>
			<if test="templateId != null and templateId != ''" >
				template_id,
			</if>
			<if test="createDate != null" >
				create_date,
			</if>
			<if test="modifyDate != null" >
				modify_date,
			</if>
	    </trim>
	</sql>
	<!-- insertParams 入库值 -->
	<sql id="insertParams">
		<trim suffix="" suffixOverrides=",">
			<if test="id != null and id != '' " >
				#{id},
			</if>
			<if test="parentNodeId != null and parentNodeId != '' " >
				#{parentNodeId},
			</if>
			<if test="nodeType != null " >
				#{nodeType},
			</if>
			<if test="nodeLevel != null " >
				#{nodeLevel},
			</if>
			<if test="nodeName != null and nodeName != ''" >
				#{nodeName},
			</if>
			<if test="templateId != null and templateId != ''" >
				#{templateId},
			</if>
			<if test="createDate != null" >
				#{createDate},
			</if>
			<if test="modifyDate != null" >
				#{modifyDate},
			</if>
	    </trim>
	</sql>
	<!-- updateParams  更新列 -->
	<sql id="updateParams">
		<trim suffix="" suffixOverrides=",">
			<if test="parentNodeId != null and parentNodeId != '' " >
				parent_node_id=#{parentNodeId},
			</if>
			<if test="nodeType != null " >
				node_type=#{nodeType},
			</if>
			<if test="nodeLevel != null " >
				node_level=#{nodeLevel},
			</if>
			<if test="nodeName != null and nodeName != ''" >
				node_name=#{nodeName},
			</if>
			<if test="templateId != null and templateId != ''" >
				template_id=#{templateId},
			</if>
			<if test="createDate != null" >
				create_date=#{createDate},
			</if>
			<if test="modifyDate != null" >
				modify_date=#{modifyDate},
			</if>
	    </trim>
	</sql>
	<!-- 条件 查询  , 去掉主键列-->
	<sql id="andOther">
		<trim suffix="" suffixOverrides=",">
			<if test="parentNodeId != null and parentNodeId != '' " >
				and parent_node_id=#{parentNodeId},
			</if>
			<if test="nodeType != null " >
				and node_type=#{nodeType},
			</if>
			<if test="nodeLevel != null " >
				and node_level=#{nodeLevel},
			</if>
			<if test="nodeName != null and nodeName != ''" >
				and node_name=#{nodeName},
			</if>
			<if test="templateId != null and templateId != ''" >
				and template_id=#{templateId},
			</if>
			<if test="createDate != null" >
				and create_date=#{createDate},
			</if>
			<if test="modifyDate != null" >
				and modify_date=#{modifyDate},
			</if>
	    </trim>
	</sql>
	<!-- 条件 查询  , 包含所有 -->
	<sql id="andOtherAll">
		<trim suffix="" suffixOverrides=",">
			<if test="id != null and id != '' " >
				and id=#{id},
			</if>
			<if test="parentNodeId != null and parentNodeId != '' " >
				and parent_node_id=#{parentNodeId},
			</if>
			<if test="nodeType != null " >
				and node_type=#{nodeType},
			</if>
			<if test="nodeLevel != null " >
				and node_level=#{nodeLevel},
			</if>
			<if test="nodeName != null and nodeName != ''" >
				and node_name=#{nodeName},
			</if>
			<if test="templateId != null and templateId != ''" >
				and template_id=#{templateId},
			</if>
			<if test="createDate != null" >
				and create_date=#{createDate},
			</if>
			<if test="modifyDate != null" >
				and modify_date=#{modifyDate},
			</if>
	    </trim>
	</sql>
	
	<!-- where 条件  -->
	<sql id="where">
	     where 1=1
	</sql>
	<!-- columnKey  主键 列名称 ,视图获取不到主键 需要设置 -->
	<sql id="columnKey">
	     and id=#{id}
	</sql>
	<!-- 自定义查询 -->
	<sql id="sql">
	   <if test="sql != null and sql !=''">
	       ${sql}
	   </if>
	   <if test="sql == null || sql==''">
	      select <include refid="tbaleColumns"/> from medical_node <include refid="where"/>
	   </if>
	</sql>
	<!-- 添加,插入记录   -->
	<insert id="insert"  parameterType="com.kungfu.dental.entity.MedicalNode" useGeneratedKeys="true">
		<!-- <selectKey keyProperty="id" resultType="String" order="BEFORE">
        	replace(uuid(),'-','')
    	</selectKey> -->
	    insert into medical_node(<include refid="insertColumns"/>) values(<include refid="insertParams"/>);
	</insert>
	<!-- 添加,SQL添加   -->
	<insert id="insertBySql" parameterType="java.lang.String">
	    <include refid="sql"/>
	</insert>
	<!-- 删除,主键删除   -->
	<delete id="deleteByPrimaryKey" parameterType="com.kungfu.dental.entity.MedicalNode">
	    delete from medical_node<include refid="where"/><include refid="columnKey"/>
	</delete>
	<!-- 删除,实体删除   -->
	<delete id="deleteByEntity" parameterType="com.kungfu.dental.entity.MedicalNode">
	    delete from medical_node<include refid="where"/><include refid="andOtherAll"/>
	</delete>
	<!-- 删除,SQL删除   -->
	<delete id="deleteBySql" parameterType="java.lang.String">
	      <include refid="sql"/>
	</delete>
	<!-- 修改,主键更新  -->
	<update id="updateByPrimaryKey" parameterType="com.kungfu.dental.entity.MedicalNode" >
		update medical_node <set><include refid="updateParams"/></set><include refid="where"/><include refid="columnKey"/>
	</update>
	<!-- 修改,SQL更新 -->
	<update id="updateBySql" parameterType="java.lang.String">
		  <include refid="sql"/>
	</update>
	<!-- 查询,主键查询   -->
	<select id="selectByPrimaryKey"  resultMap="medicalNodeMap" parameterType="java.lang.Object">
		select <include refid="tbaleColumns"/> from medical_node <include refid="where"/> <include refid="columnKey"/>
	</select>
	<!-- 查询,所有   -->
	<select id="selectAll"  resultMap="medicalNodeMap">
		select <include refid="tbaleColumns"/> from medical_node
	</select>
	<!-- 查询,总数量   -->
	<select id="count" resultType="java.lang.Integer">
		select count(1) from medical_node
	</select>
	<!-- 查询,实体查询   -->
	<select id="selectByEntiry"  resultMap="medicalNodeMap" parameterType="com.kungfu.dental.entity.MedicalNode">
		select <include refid="tbaleColumns"/> from medical_node <include refid="where"/> <include refid="andOtherAll"/>
	</select>
	<!-- 查询,SQL -->
	<select id="selectBySql"  resultMap="medicalNodeMap" parameterType="java.lang.String">
	     <include refid="sql"/>
	</select>
	<!-- 查询,总行数,Map -->
	<select id="selectByMapCount" resultType="java.lang.Integer"  parameterType="java.util.Map">
		select count(1) from medical_node <include refid="where"/>
	    ${queryCondition}
		<if test="andCondition !=null and andCondition !=''">
		   and ${andCondition}
		</if>
		<if test="orderCondition != null and queryCondition != ''" >
	    	order by ${orderCondition}
	    </if>
	</select>
	<!-- 查询,参数查询,Map -->
	<select id="selectByMap" resultMap="medicalNodeMap"  parameterType="java.util.Map">
		select <include refid="tbaleColumns"/>from medical_node <include refid="where"/>
	    ${queryCondition}
		<if test="andCondition !=null and andCondition !=''">
		   and ${andCondition}
		</if>
		<if test="orderCondition != null and orderCondition != ''" >
	    	order by ${orderCondition}
	    </if>
	</select>
</mapper>   
