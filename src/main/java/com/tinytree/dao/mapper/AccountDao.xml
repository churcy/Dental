	<sql id="columnKey">
	     and id=#{id}
	</sql>
	<!-- 自定义查询 -->
	<sql id="sql">
	   <if test="sql != null and sql !=''">
	       ${sql}
	   </if>
	   <if test="sql == null || sql==''">
	      select <include refid="tbaleColumns"/> from account <include refid="where"/>
	   </if>
	</sql>
	<!-- 添加,插入记录   -->
	<insert id="insert"  parameterType="com.kungfu.dental.entity.Account" useGeneratedKeys="true">
		<!-- <selectKey keyProperty="id" resultType="String" order="BEFORE">
        	replace(uuid(),'-','')
    	</selectKey> -->
	    insert into account(<include refid="insertColumns"/>) values(<include refid="insertParams"/>);
	</insert>
	<!-- 添加,SQL添加   -->
